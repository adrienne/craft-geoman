{
  "name": "handplant/craft-geoman",
  "description": "geometry editing tools",
  "type": "craft-plugin",
  "version": "1.0.1",
  "license": "MIT",
  "keywords": [
    "craft",
    "cms",
    "craftcms",
    "craft-plugin",
    "craft geoman",
    "craft field"
  ],
  "support": {
    "docs": "https://github.com/handplant/craft-geoman/blob/master/README.md",
    "issues": "https://github.com/handplant/craft-geoman/issues"
  },
  "authors": [
    {
      "name": "webworker",
      "homepage": "https://webworker.me"
    }
  ],
  "require": {
    "php": "^8.0",
    "craftcms/cms": "^4.0.0"
