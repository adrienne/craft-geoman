  "autoload": {
    "psr-4": {
      "handplant\\craftgeoman\\": "src/"
    }
  },
  "extra": {
    "name": "Craft Geoman",
    "handle": "craft-geoman",
    "developer": "webworker",
    "developerUrl": "https://webworker.me",
    "documentationUrl": "https://github.com/handplant/craft-geoman/blob/master/README.md",
    "changelogUrl": "https://raw.githubusercontent.com/handplant/craft-geoman/master/CHANGELOG.md",
    "class": "handplant\\craftgeoman\\CraftGeoman"
  },
  "config": {
    "allow-plugins": {
      "yiisoft/yii2-composer": true,
      "craftcms/plugin-installer": true
    }
  }
}
